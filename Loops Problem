#include<iostream>

using namespace std;

int main()

{   int rows, cols, i,j;

    cout << "Please Enter Rows: "<<endl;

    cin >>rows;

    cout <<"Please Enter Cols: "<<endl;

    cin>>cols;

    for(i=1;i<=rows;i++){

         for(j=1;j<=cols;j++){

         cout<<"*";

        }   

        cout<<endl;

    }

    return 0;

}
